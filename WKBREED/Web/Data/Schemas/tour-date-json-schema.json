{
	"$schema": "http://json-schema.org/draft-04/schema",
	"title": "JSON Schema for tour dates JSON file format",
	"type": "object",
	"required": [
		"tours"
	],
	"properties": {
		"tours": {
			"$id": "#/properties/tours",
			"type": "array",
			"title": "The Tours Schema",
			"items": {
				"$id": "#/properties/tours/items",
				"type": "object",
				"title": "The Items Schema",
				"required": [
					"venue",
					"Location",
					"Date",
					"TicketUrl"
				],
				"properties": {
					"Venue": {
						"$id": "#/properties/tours/items/properties/Venue",
						"type": "string",
						"title": "The Venue Schema",
						"default": "",
						"examples": [
							"BoxPark"
						],
						"pattern": "^(.*)$"
					},
					"Location": {
						"$id": "#/properties/tours/items/properties/Location",
						"type": "string",
						"title": "The Location Schema",
						"default": "",
						"examples": [
							"London, GB"
						],
						"pattern": "^(.*)$"
					},
					"Date": {
						"$id": "#/properties/tours/items/properties/Date",
						"type": "string",
						"title": "The Date Schema",
						"default": "",
						"examples": [
							"01/04/2019"
						],
						"pattern": "^(.*)$"
					},
					"TicketUrl": {
						"$id": "#/properties/tours/items/properties/TicketUrl",
						"type": "string",
						"title": "The Ticketurl Schema",
						"default": "",
						"examples": [
							"https://wkbreed.com/"
						],
						"pattern": "^(.*)$"
					}
				}
			}
		}
	}
}